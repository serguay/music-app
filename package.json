{
  "name": "music-app",
  "private": true,
  "version": "1.4.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.sergio.musicapp",
    "productName": "Music App",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/icon.ico"
    }
  },
  "dependencies": {
    "@primevue/themes": "^4.5.4",
    "@supabase/supabase-js": "^2.89.0",
    "pinia": "^3.0.4",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3",
    "vite": "^7.2.4"
  }
}
