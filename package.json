{
  "name": "music-app",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.sergio.musicapp",
    "productName": "Music App",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/icon.ico"
    }
  },
  "dependencies": {
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@lottiefiles/dotlottie-vue": "^0.10.12",
    "@lottiefiles/vue-lottie-player": "^1.1.0",
    "@primevue/themes": "^4.5.4",
    "@supabase/supabase-js": "^2.89.0",
    "avataaars": "^2.0.0",
    "canvas-confetti": "^1.9.4",
    "idb": "^8.0.3",
    "leaflet": "^1.9.4",
    "maplibre-gl": "^5.17.0",
    "pinia": "^3.0.4",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.6.0",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0"
  }
}
